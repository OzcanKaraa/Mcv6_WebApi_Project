@model IEnumerable<Employee>

<br />
<br />
<table class="table">
    <tdead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>City</th>
        </tr>
    </tdead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FName</td>
                <td>@item.LName</td>
                <td>@item.City</td>
            </tr>
        }
    </tbody>
</table>